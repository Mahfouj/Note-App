<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/dicotor"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.myreycycle.homeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_dcotorFragment"
            app:destination="@id/dcotorFragment" />
    </fragment>
    <fragment
        android:id="@+id/dcotorFragment"
        android:name="com.example.myreycycle.dcotorFragment"
        android:label="fragment_dcotor"
        tools:layout="@layout/fragment_dcotor" />
</navigation>